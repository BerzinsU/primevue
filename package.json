{
    "name": "@primevue/monorepo",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "styled:build": "pnpm --filter styled build",
        "apidoc:build": "pnpm --filter apidoc build",
        "dev": "pnpm --filter showcase dev",
        "module:dev": "pnpm --filter @primevue/nuxt-module dev",
        "build:check": "pnpm run format:check && pnpm run security:check",
        "security:check": "pnpm audit --production --audit-level high",
        "format": "prettier --write \"**/*.{vue,js,ts,d.ts}\" --cache",
        "format:check": "prettier --check \"**/*.{vue,js,ts,d.ts}\"",
        "lint": "eslint --ext \".vue,.js,.ts\" --ignore-path .gitignore . --cache",
        "lint:fix": "eslint --fix --ext \".vue,.js,.ts\" --ignore-path .gitignore ."
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^9.2.0",
        "prettier": "^3.2.5",
        "pnpm": "9.1.4"
    }
}
