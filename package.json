{
    "name": "@primevue/monorepo",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "build": "NODE_ENV=production pnpm run build:check && pnpm run build:packages",
        "build:check": "pnpm run format:check && pnpm run security:check",
        "build:packages": "pnpm run build:resolver && pnpm run build:core && pnpm run build:module && pnpm run build:lib && pnpm run build:themes",
        "build:resolver": "pnpm --filter auto-import-resolver build",
        "build:core": "pnpm --filter core build",
        "build:module": "pnpm --filter nuxt-module build",
        "build:lib": "pnpm --filter primevue build",
        "build:themes": "pnpm --filter themes build",
        "dev": "pnpm --filter showcase dev",
        "module:dev": "pnpm --filter @primevue/nuxt-module dev",
        "security:check": "pnpm audit --production --audit-level high",
        "format": "prettier --write \"**/*.{vue,js,ts,d.ts}\" --cache",
        "format:check": "prettier --check \"**/*.{vue,js,ts,d.ts}\"",
        "lint": "eslint --ext \".vue,.js,.ts\" --ignore-path .gitignore . --cache",
        "lint:fix": "eslint --fix --ext \".vue,.js,.ts\" --ignore-path .gitignore ."
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^9.2.0",
        "prettier": "^3.2.5",
        "pnpm": "9.1.4"
    }
}
